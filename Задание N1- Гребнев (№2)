Program N12;
Var a,b,n:integer;
Begin
    a:=random(1,99);
    n:=0;
    Writeln(a);
    While (a<>b) and (n<>6) 
    Do begin Write('Введите число:');
            Readln(b);
            n:=n+1;
    If a<b 
    then Writeln('Less ↓');
    If a>b
    then Writeln('More ↑');
       end; 
    If a=b 
    then Writeln('WIN!')
    else Writeln('LOSE...')
End.
